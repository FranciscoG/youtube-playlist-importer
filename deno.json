{
  "imports": {
    "std/": "https://deno.land/std@0.207.0/"
  },
  "tasks": {
    "run": "deno run --allow-net=www.googleapis.com,deno.land --allow-read=. main.ts"
  },
  "fmt": {
    "useTabs": true,
    "lineWidth": 80,
    "indentWidth": 2,
    "semiColons": true,
    "singleQuote": true,
    "include": ["./*.ts"]
  }
}
